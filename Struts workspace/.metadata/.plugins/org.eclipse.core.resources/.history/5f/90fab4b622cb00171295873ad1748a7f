	int count=0;
	private List<String> checker(String s) {
		List<String> sugWords = new ArrayList<String>();
		for (int i = 0; i < words.size(); i++) {
			if (s.equals(words.get(i))) {
				count ++;
			}
			if(count>0){
				
			}
		}
		return words;
	}
	
	public static void main(String[] args) {
		SpellChecker sc = new SpellChecker();
		 sc.checker("phone");
		
	}

}
