package inheritance;

public class Substrings {

	public static void main(String[] args) {
		String s1= "coke";
		String s2="keco";
		int s2fristIndex = 0;
		char firstChar = s1.charAt(0);
		System.out.println(firstChar);
		for (int i = 0; i < s2.length(); i++) {
			if(firstChar == s2.charAt(i)){
				s2fristIndex = i;
			}
		}
		System.out.println(s2fristIndex);
		
	}
}
