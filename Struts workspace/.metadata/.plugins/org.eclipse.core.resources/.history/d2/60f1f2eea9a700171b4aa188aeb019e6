package inheritance;

import java.util.Scanner;

public class NumberLetter {

	public static void main(String[] args) {
		System.out.println("Pls enter the string:");
		Scanner sc = new Scanner(System.in);
		String inpStrng = sc.nextLine();
		String outString = "";

		for (int i = 0; i < inpStrng.length(); i++) {
			if (inpStrng.charAt(i) == 97) {
				outString = outString + 0;
			}
			if (inpStrng.charAt(i) == 98) {
				outString = outString + 1;
			}
			if (inpStrng.charAt(i) == 99) {
				outString = outString + 2;
			}
			if (inpStrng.charAt(i) == 100) {
				outString = outString + 3;
			}
			if (inpStrng.charAt(i) == 101) {
				outString = outString + 4;
			}
			if (inpStrng.charAt(i) == 102) {
				outString = outString + 5;
			}
			if (inpStrng.charAt(i) == 103) {
				outString = outString + 6;
			}
			if (inpStrng.charAt(i) == 104) {
				outString = outString + 7;
			}
			if (inpStrng.charAt(i) == 105) {
				outString = outString + 8;
			}
			if (inpStrng.charAt(i) == 106) {
				outString = outString + 9;
			}
		}
		System.out.println(outString);
		String[] vals = outString.split(".");
		for (int i = 0; i < vals.length; i++) {
			System.out.println(vals[i]);
		}
	}

}
