package inheritance;

import java.util.Scanner;

public class NumberLetter {

	public static void main(String[] args) {
		System.out.println("Pls enter the string:");
		Scanner sc = new Scanner(System.in);
		String inpStrng = sc.nextLine();
		String outString = "";
		
		for (int i = 0; i < inpStrng.length(); i++) {
			for (int j = 97; j <=106 ; j++) {
				if(inpStrng.charAt(i)==j){
					outString = outString + "."+j;
				}	
			}
		}
		System.out.println(outString);
		
	}

}
